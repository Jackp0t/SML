[%- SET basename = self.get_basename -%]
[%- SET width = self.get_width -%]
[%- SET height = self.get_height -%]
[%- IF width > 605 -%]
<div align="center">
<a href="images/[%basename%]">
<img src="images-scaled/[%basename%]" width="605" border="0">
</a>
</div>
[%- ELSE -%]
<div align="center">
<img src="images/[%basename%]" width="[%width%]" height="[%height%]">
</div>
[%- END -%]
